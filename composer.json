{
  "name": "drupal/gsb_feature_wysiwyg",
  "description": "GSB Feature Wysiwyg",
  "license": "Apache-2.0",  
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "ckeditor/ckeditor",
        "version": "master",
        "type": "drupal-gsb-library", 
        "dist": {
          "url": "https://download.cksource.com/CKEditor/CKEditor/CKEditor%204.4.7/ckeditor_4.4.7_standard.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "markitup/markitup",
        "version": "1.1.14",
        "type": "drupal-gsb-library", 
        "source": {
          "url": "https://github.com/markitup/1.x.git",
          "type": "git",
          "reference": "1.1.14"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "markdown/markdown",
        "version": "master",
        "type": "drupal-gsb-library", 
        "dist": {
          "url": "https://markitup.jaysalvat.com/downloads/download.php?id=markupsets/markdown",
          "type": "zip"
        }
      }
    }
  ],  
  "require": {
    "php": ">=5.3.0",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-xml": "*",
    "cweagans/composer-patches": "^1.6.5",
    "drupal/wysiwyg": "2.x-dev#40b26a1",
    "drupal/wysiwyg_filter": "1.6-rc2",
    "drupal/linkit": "2.7",
    "drupal/image_resize_filter": "1.16",
    "drupal/caption_filter": "1.3",
    "drupal/footnotes": "2.x-dev#7b12c77",
    "drupal/features_override": "2.0-rc1",
    "ckeditor/ckeditor": "master",
    "markitup/markitup": "1.1.14",
    "markdown/markdown": "master"
  },
  "extra": {
    "installer-paths": {
      "web/profiles/gsb_public/libraries/{$name}/": ["type:drupal-library"]
    },
    "patches": {
      "cweagans/composer-patches": {
        "Call the preserve paths hooks" : "https://github.com/SebCorbin/composer-patches/commit/1cb9bacba51f8bba0c5f3f322c3bb61e4ceae974.patch"
      },
      "drupal/wysiwyg": {
        "397994": "https://www.drupal.org/files/issues/397994-d7-32.patch"  
      },
      "drupal/footnotes": {
        "2296211": "https://www.drupal.org/files/issues/2296211-d7-2.patch"  
      },
      "markitup/markitup": {
        "WP-5186": "https://raw.githubusercontent.com/gsb-public/gsb-revamp-patches/master/WP-5186-jquery-markitup.js.patch"  
      }
    }
  }
}
